@model List<EF.Models.Database.Movy>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div>
    <table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Name</th>

        </tr>
         @foreach (var mv in Model)
        {
        <tr>
            <td>@mv.id</td>
            <td>@mv.name</td>
        </tr>
        }

    </table>
</div>

